#include <iostream>

using namespace std;


int isDigit(string input){
    for(int i=0;i<input.length();i++){
        if(isdigit(input[i])){
            return 1;
        }
    }
    
    return 0;
    
}


int main() {

    string input = "";
    cin>>input;
    
    if(isDigit(input)){
        cout<<"Input has a number!";
    }
    else{
        cout<<"Input is not a number!";
    }



    return 0;
}
